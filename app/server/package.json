  "description": "Node Server for Web Pipeline For Flux Balance Analysis (Constraint-Based-Models)",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:development": "$(yarn bin)/nodemon index.js",
    "start:docker": "$(yarn bin)/pm2-runtime start index.js",
    "start": "yarn run start:development"
  },
  "nodemonConfig": {
    "ignore": [
      "analysis/*"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "eslint-config-prettier": "^5.0 .0",
    "express": "^4.17.3",
    "helmet": "^3.18.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "pm2": "^3.5.1",
    "python-shell": "^1.0.7",
    "sequelize": "^5.8.7"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "nodemon": "^1.19.1"
  }
}
